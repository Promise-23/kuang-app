
page{background-color: #f6f6f6;}
.search-back{
	background-color: #FFFFFF;
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	z-index: 99;
}
.search-input{
	background-color: #FFFFFF;
	display: flex;
	align-items: center;
	justify-content: space-around;
	margin-bottom: 5px;
}
.tab-jiantou{
	padding: 30rpx 20rpx;
	width: 50rpx;
	height: 50rpx;
}
.tab-jiantou image{
	width: 100%;
	height: 100%;
}
.tab-view{
	display: flex;
	flex-direction: column;
	align-items: center;
	position: relative;
}
.tab-view text:nth-child(2){
	width: 50rpx;
	height: 8rpx;
	border-radius: 10rpx;
	position: absolute;
	bottom: 0;
}
.active{
	background-color: rgba(255, 85,0, 1);
}

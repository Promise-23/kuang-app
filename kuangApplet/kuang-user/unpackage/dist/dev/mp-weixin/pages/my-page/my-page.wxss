
.my-page.data-v-d9593e3f{
	height:350rpx;
	width: calc(100% - 80rpx);
	padding: 20rpx;
	margin: 0 auto;
	background-image: linear-gradient(to bottom right, #000 0%, #575757 90%, #575757 100%);
	/* background-image: linear-gradient(to bottom right, #EEDABC 0%, #987952 100%, #022F2C 100%); */
	position: relative;
	border-radius: 16rpx;
	box-shadow: 6px 6px 5px rgba(0,0,0,0.2);
}
.user-info.data-v-d9593e3f {
	display: flex;
	justify-content: space-between;
}
.user-info image.data-v-d9593e3f{
	display: block;
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
	margin-bottom: 10rpx;
}
.user-info .info.data-v-d9593e3f{
	color: #FFFFFF;
	display: flex;
	align-items: center;
	/* justify-content: center; */
	margin-left: 50rpx;
	margin-top: 50rpx;
	font-size: 33rpx;
	z-index: 9;
	background: linear-gradient(to bottom right, #EEDABC 0%, #987952 100%);
	-webkit-background-clip: text;
	-webkit-text-fill-color: transparent;
}
.user-info .logout image.data-v-d9593e3f{
	width: 40rpx;
	height: 40rpx;
}
.propertys.data-v-d9593e3f{
	margin-top: 40rpx;
	display: flex;
	align-items: flex-end;
	justify-content: space-around;
}
.propertys .box.data-v-d9593e3f{
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	font-size: 14px;
	background: linear-gradient(to bottom right, #EEDABC 0%, #987952 100%);
	-webkit-background-clip: text;
	-webkit-text-fill-color: transparent;
}
.propertys .box image.data-v-d9593e3f{
	height: 50rpx;
	width: 50rpx;
}
.propertys .integral image.data-v-d9593e3f{
	height: 40rpx;
	width: 40rpx;
}
.propertys .box .num.data-v-d9593e3f{
	font-size: 18px;
	font-weight: 600;
	margin-top: 6rpx;
}
.text-info.data-v-d9593e3f{
	display: flex;
	flex-direction: column;
	margin-left: 20rpx;
}
.text-info .nickname.data-v-d9593e3f{
	font-size: 14px;
}
.text-info .phone.data-v-d9593e3f{
	font-size: 12px;
	margin-top: 3px;
}
.text-info .grade.data-v-d9593e3f{
	font-size: 12px;
}
.shuibo-img.data-v-d9593e3f{
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 170rpx;
	/* z-index: 99; */
	mix-blend-mode: screen;
}
/* 我的订单 */
.my-order.data-v-d9593e3f{
	margin-top: 20rpx;
	padding: 0 20rpx;
}
.my-order-title image.data-v-d9593e3f{
	width: 30rpx;
	height: 30rpx;
	display: block;
	margin-left: 10rpx;
}
.my-order-title.data-v-d9593e3f{
	display: flex;
	justify-content: space-between;
}
.my-order-title view.data-v-d9593e3f:nth-child(1){
	font-weight: bold;
}
.my-order-title view.data-v-d9593e3f:nth-child(2){
	font-size: 25rpx;
	color: #cfcfcf;
}
.more.data-v-d9593e3f{
	display: flex;
	align-items: center;
}
.order-state image.data-v-d9593e3f{
	display: block;
	width: 50rpx;
	height: 50rpx;
	margin-bottom: 10rpx;
}
.order-state.data-v-d9593e3f{
	display: flex;
	align-items: center;
	justify-content: space-between;
	margin-top: 30rpx;
}
.order-state view.data-v-d9593e3f{
	display: flex;
	align-items: center;
	flex-direction: column;
	color: #4f4f4f;
}
/* 其他功能 */
.my-other.data-v-d9593e3f{
	background-color: #FFFFFF;
	margin: 50rpx 20rpx 20rpx 20rpx;
	box-shadow: rgba(100, 100, 111, 0.2) 0px 7px 29px 0px;
	border-radius: 6rpx;
	padding: 0 10rpx;
	color: #828282;
}
.my-other image.data-v-d9593e3f{
	display: block;
	width: 35rpx;
	height: 35rpx;
	margin-right: 20rpx;
}
.my-other view.data-v-d9593e3f{
	display: flex;
	align-items: center;
	padding: 30rpx 0;
}
.my-other view.data-v-d9593e3f:nth-child(2){
	border-bottom: 1rpx solid #e0e0e0;

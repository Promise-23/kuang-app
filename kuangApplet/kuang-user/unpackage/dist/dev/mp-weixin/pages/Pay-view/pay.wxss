
page{
		background-color: #f6f6f6;
}


.card.data-v-b018696c{
	box-sizing: border-box;
	width: 96%;
	margin: 0 auto;
	padding: 20rpx;
	margin-top: 20rpx;
	border-radius: 12rpx;
	background-color: #FFFFFF;
}
.pay-address.data-v-b018696c{
	display: flex;
	align-items: center;
}
.pay-address-left image.data-v-b018696c{
	display: block;
	width: 40rpx;
	height: 40rpx;
}
.pay-address-right image.data-v-b018696c{
	display: block;
	width: 30rpx;
	height: 30rpx;
}
.pay-address-name.data-v-b018696c{
	flex: 1;
	padding: 0 20rpx;
}
.pay-address-name view.data-v-b018696c{
	display: flex;
	align-items: center;
}
.pay-address-name view text.data-v-b018696c:nth-child(1){
	padding-right: 10rpx;
	font-weight: bold;
}
.pay-address-name view text.data-v-b018696c:nth-child(2){
	color: #8b8b8d;
}
.placeholder-tip.data-v-b018696c{
	color: #8b8b8d;
}
/* 待支付商品 */
.pay-goods.data-v-b018696c{
	display: flex;
	padding: 20rpx 20rpx 40rpx 20rpx;
}
.pay-goods text.data-v-b018696c{
	display: block;
}
.pay-goods image.data-v-b018696c{
	display: block;
	width: 200rpx;
	height: 200rpx;
	margin-right: 20rpx;
	border-radius: 10rpx;
}
.pay-goods-title.data-v-b018696c{
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box !important;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}
.pay-goods-specs.data-v-b018696c{
	background-color: #fafafa;
	padding: 10rpx;
	display: inline-block !important;
	font-size: 28rpx;
	color: #a4a4a4;
	margin-top: 20rpx;
	border-radius: 10rpx;
}
.pay-goods-price.data-v-b018696c{
	padding-top: 40rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	font-weight: bold;
}
.pay-goods-price image.data-v-b018696c{
	width: 50rpx;
	height: 50rpx;
	display: block;
	margin: 0 !important;
}
.pay-goods-price>text.data-v-b018696c{
	color: #fc324a;
	margin-right: 40rpx;
}
.pay-goods-price view.data-v-b018696c{
	display: flex;
}
.pay-goods-price view text.data-v-b018696c{
	padding: 0 20rpx;
}

/*资产*/
.line.data-v-b018696c{
	display: flex;
	justify-content: space-between;
	font-size: 12px;
	margin-bottom: 10rpx;
	color: #8b8b8d;
}
.line.data-v-b018696c:last-child{
	margin-bottom: 0;
}
.line image.data-v-b018696c{
	width: 30rpx;
	height: 30rpx;
	margin-left: 10rpx;
}
.line .right.data-v-b018696c{
	display: flex;
	align-items: center;
}
/* 结算 */
.set-accounts.data-v-b018696c{
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding: 20rpx 20rpx 68rpx 20rpx;
}
.set-accounts view.data-v-b018696c:nth-child(1){
	color: #fc324a;
	font-size: 35rpx;
	font-weight: bold;
}
.set-accounts view.data-v-b018696c:nth-child(2){
	background-color: #FF5500;
	color: #FFFFFF;
	font-size: 35rpx;
	padding: 15rpx 35rpx;
}
.hascount.data-v-b018696c{
	color: #FF5500;
	margin-right: 5rpx;
	font-size: 14px;
	margin-left: 20rpx;
}
